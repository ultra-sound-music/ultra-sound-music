{
  "root": "libs/copy",
  "sourceRoot": "libs/copy/src",
  "projectDir": "",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/copy/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/copy"
      ],
      "options": {
        "jestConfig": "libs/copy/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
