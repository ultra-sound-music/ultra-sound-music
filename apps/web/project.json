{
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "projectDir": "",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/web",
        "optimization": false,
        "index": "apps/web/public/index.html",
        "generateIndexHtml": false,
        "baseHref": "/",
        "main": "apps/web/src/main.tsx",
        "polyfills": "apps/web/src/polyfills.ts",
        "tsConfig": "apps/web/tsconfig.app.json",
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/web/build/webpack.js"
      },
      "configurations": {
        "development": {
          "assets": [
            {
              "input": "libs/assets/src/img/favicons",
              "glob": "*.*",
              "output": "."
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/config/src/environment.ts",
              "with": "libs/config/src/environment.dev.ts"
            }
          ]
        },
        "stage": {
          "assets": [
            {
              "input": "libs/assets/src/img/favicons",
              "glob": "*.*",
              "output": "."
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/config/src/environment.ts",
              "with": "libs/config/src/environment.stage.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production": {
          "assets": [
            {
              "input": "apps/web/src/assets/prod",
              "glob": "*.*",
              "output": "."
            },
            {
              "input": "libs/assets/src/img/favicons",
              "glob": "*.*",
              "output": "."
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/config/src/environment.ts",
              "with": "libs/config/src/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "web:build:development",
        "hmr": true
      },
      "configurations": {
        "stage": {
          "buildTarget": "web:build:stage",
          "hmr": false
        },
        "production": {
          "buildTarget": "web:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "web:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/web/**/*.css", "apps/web/**/*.scss"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/web"],
      "options": {
        "jestConfig": "apps/web/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
